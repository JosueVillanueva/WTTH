<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Bean SYSTEM "bean.dtd">
<Bean>
	<Name>Network Element</Name>
	<Solution>WTTH</Solution>
	<Package>com.hpe.ov.activator.inventory</Package>
	<DBTable>GENSERV_Network_Element</DBTable>
	<Fields>
		<Field mandatory="true">
			<Name>ip_address</Name>
			<Type>string</Type>
			<Description>ip address</Description>
		</Field>
		<Field mandatory="false">
			<Name>network_element_type</Name>
			<Type>string</Type>
			<Description>NE primary key</Description>
		</Field>
		<Field mandatory="false">
			<Name>device_name</Name>
			<Type>string</Type>
			<Description>device_name</Description>
		</Field>
		<Field mandatory="false">
			<Name>port_number</Name>
			<Type>string</Type>
			<Description>port number</Description>
		</Field>
		<Field mandatory="false">
			<Name>user_name</Name>
			<Type>string</Type>
			<Description>user name</Description>
		</Field>
		<Field mandatory="false">
			<Name>password</Name>
			<Type>string</Type>
			<Description>password</Description>
		</Field>
	</Fields>
	<Keys>
		<Key pk="true">
			<KeyField>ip_address</KeyField>
		</Key>
	</Keys>
</Bean>
