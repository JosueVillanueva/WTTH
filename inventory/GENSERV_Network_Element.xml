<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Bean SYSTEM "bean.dtd">
<Bean>
	<Name>NetworkElement</Name>
	<Solution>WTTH</Solution>
	<Package>com.hpe.ov.activator.inventory</Package>
	<DBTable>GENSERV_Network_Element</DBTable>
	<Fields>
		<Field mandatory="true">
			<Name>ipAddress</Name>
			<Column>IP_ADDRESS</Column>
			<Type>String</Type>
			<Description>IP address</Description>
		</Field>
		<Field mandatory="false">
			<Name>networkElementType</Name>
			<Column>NETWORK_ELEMENT_TYPE</Column>
			<Type>String</Type>
			<Description>NE primary key</Description>
		</Field>
		<Field mandatory="false">
			<Name>deviceName</Name>
			<Column>DEVICE_NAME</Column>
			<Type>String</Type>
			<Description>device name</Description>
		</Field>
		<Field mandatory="false">
			<Name>portNumber</Name>
			<Column>PORT_NUMBER</Column>
			<Type>String</Type>
			<Description>port number</Description>
		</Field>
		<Field mandatory="false">
			<Name>userName</Name>
			<Column>USER_NAME</Column>
			<Type>String</Type>
			<Description>user name</Description>
		</Field>
		<Field mandatory="false">
			<Name>password</Name>
			<Column>PASSWORD</Column>
			<Type>String</Type>
			<Description>password</Description>
		</Field>
	</Fields>
	<Keys>
		<Key pk="true">
			<KeyField>ipAddress</KeyField>
		</Key>
	</Keys>
	<Operations>
		<Store/>
		<Update/>
		<Remove/>
		<FindBy>
			<OrderField>ipAddress</OrderField>
		</FindBy>
	</Operations>
</Bean>
