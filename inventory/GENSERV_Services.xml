<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Bean SYSTEM "bean.dtd">
<Bean>
	<Name>Services</Name>
	<Solution>WTTH</Solution>
	<Package>com.hpe.ov.activator.inventory</Package>
	<DBTable>GENSERV_Services</DBTable>
	<Fields>
		<Field mandatory="true" upDate="false">
			<Name>serviceId</Name>
			<Column>SERVICE_ID</Column>
			<Type>String</Type>
			<Description>service primary key</Description>
		</Field>
		<Field mandatory="false">
			<Name>customerId</Name>
			<Column>COSTUMER_ID</Column>
			<Type>String</Type>
			<Description>customer id</Description>
		</Field>
		<Field mandatory="false">
			<Name>serviceType</Name>
			<Column>SERVICE_TYPE</Column>
			<Type>String</Type>
			<Description>service type</Description>
		</Field>
		<Field mandatory="false">
			<Name>accountNumber</Name>
			<Column>ACCOUNT_NUMBER</Column>
			<Type>String</Type>
			<Description>account number</Description>
		</Field>
		<Field mandatory="false">
			<Name>accountStatus</Name>
			<Column>ACCOUNT_STATUS</Column>
			<Type>String</Type>
			<Description>account_status</Description>
		</Field>
		<Field mandatory="false">
			<Name>aux1</Name>
			<Column>AUX1</Column>
			<Type>Long</Type>
			<Description>auxiliar field</Description>
		</Field>
		<Field mandatory="false">
			<Name>metada</Name>
			<Column>METADATA</Column>
			<Type>Long</Type>
			<Description>additional info</Description>
		</Field>
	</Fields>
	<Keys>
		<Key pk="true">
			<KeyField>SERVICE_ID</KeyField>
		</Key>
		<Key unique="false" makeIndex="true">
			<Name>customerId</Name>
			<KeyField>CUSTOMER_ID</KeyField>
		</Key>
		<Key unique="false" makeIndex="true">
			<Name>accountNumber</Name>
			<KeyField>ACCOUNT_NUMBER</KeyField>
		</Key>
	</Keys>
	<Operations>
		<Store/>
		<UpDate/>
		<Remove/>
		<FindBy>
			<OrderField>SERVICE_ID</OrderField>
		</FindBy>
		<FindBy key="customerId">
			<OrderField>SERVICE_ID</OrderField>
		</FindBy>
		<FindBy key="accountNumber">
			<OrderField>SERVICE_ID</OrderField>
		</FindBy>
	</Operations>
</Bean>
